#include <iostream>
using namespace std;
int main()
{
        int len;
        string n;
 
        cout<<"ENTER BINARY NUMBER: ";
        cin>>n;
 
        len=n.length();

 
        cout<<"\nthe length of string is "<<len;
 
        return 0;
}

// Output
//ENTER BINARY NUMBER: 1100
//the length of string is 4
